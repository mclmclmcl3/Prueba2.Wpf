        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pureba2.Wpf.Views"
        xmlns:v="clr-namespace:Pureba2.Wpf.Views"
        xmlns:vm="clr-namespace:Pureba2.Wpf.ViewModels"
        mc:Ignorable="d"
        Title="InicioView" Height="450" Width="800">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <v:LateralView Grid.Column="0"/>
        <v:CentralView Grid.Column="1"/>
    </Grid>
</Window>
